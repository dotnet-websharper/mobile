<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true" />
    <httpModules>
      <add name="RpcModule" type="IntelliFactory.WebSharper.Web.RpcModule, IntelliFactory.WebSharper.Web"/>
      <add name="IntelliFactory.WebSharper.Sitelets" type="IntelliFactory.WebSharper.Sitelets.HttpModule, IntelliFactory.WebSharper.Sitelets"/>
    </httpModules>
    <pages>
      <controls>
        <add tagPrefix="WebSharper" namespace="IntelliFactory.WebSharper.Web" assembly="IntelliFactory.WebSharper.Web"/>
      </controls>
    </pages>
    
    <authentication mode="Forms">
      <forms loginUrl="Login/None"/>
    </authentication>
    
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false"/>
  </system.webServer>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true">
      <add name="RpcModule" type="IntelliFactory.WebSharper.Web.RpcModule, IntelliFactory.WebSharper.Web"/>
      <add name="IntelliFactory.WebSharper.Sitelets" type="IntelliFactory.WebSharper.Sitelets.HttpModule, IntelliFactory.WebSharper.Sitelets"/>
    </modules>
  </system.webServer>
  <system.diagnostics>
    <switches>
      <add name="WebSharper.Verbosity" value="Warning" />
    </switches>
    <sources>
      <source name="WebSharper" switchName="WebSharper.Verbosity" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <remove name="Default" />
          <add name="File" />
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="File" type="System.Diagnostics.TextWriterTraceListener" initializeData="App_Data\websharper.log" />
    </sharedListeners>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="File" />
        <remove name="Default" />
      </listeners>
    </trace>
  </system.diagnostics>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1" appliesTo="v4.0.30319">
      <dependentAssembly>
        <assemblyIdentity name="FSharp.Core" publicKeyToken="b03f5f7f11d50a3a" culture="neutral"/>
        <!-- Redirecting to version 4.0.0.0 of the assembly. -->
        <bindingRedirect oldVersion="2.0.0.0" newVersion="4.0.0.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
